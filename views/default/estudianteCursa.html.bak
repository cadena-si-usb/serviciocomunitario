{{ extend 'base.html' }}
{{ block contenido }}
<!DOCTYPE html>
<!-- saved from url=(0039)http://getbootstrap.com/examples/theme/ -->
<html lang="en">
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">

    <link rel="icon" href="http://getbootstrap.com/favicon.ico">

    <link rel="stylesheet" href="{{=URL('static', 'css/font-awesome.min.css')}}">

    <link rel="stylesheet" href="{{=URL('static', 'css/bootstrap.min.css')}}">
    <link rel="stylesheet" href="{{=URL('static', 'css/Pag_principal.css')}}%}">
    <link rel="stylesheet" href="{{=URL('static', 'css/proponentes.css')}}">

    <link rel="icon" href="//getbootstrap.com/favicon.ico">



    <!-- Bootstrap core CSS -->
    <link href="{{=URL('static', 'css/bootstrap.css')}}" rel="stylesheet">
    <!-- Bootstrap theme -->
    <link href="{{=URL('static', 'css/bootstrap-theme.css')}}" rel="stylesheet">

    <script type="text/javascript" src="{{=URL('static', 'js/jquery-1.11.2.js')}}">
    </script>
    <script type="text/javascript" src="{{=URL('static', 'js/web2py.js')}}">
    </script>


    <!-- Custom styles for this template -->

    <link href="{{=URL('static', 'css/theme.css')}}" rel="stylesheet">
    <link href="{{=URL('static', 'js/bootstrap.js')}}" rel="stylesheet">

    <link href="{{=URL('static', 'fonts/glyphicons-halflings-regular.svg', scheme='http')}}" rel="stylesheet"/>
    <link href="{{=URL('static', 'fonts/glyphicons-halflings-regular.eot', scheme='http')}}" rel="stylesheet" />

    <link href="{{=URL('static', 'fonts/glyphicons-halflings-regular.ttf', scheme='http')}}" rel="stylesheet" />
    <link href="{{=URL('static', 'fonts/glyphicons-halflings-regular.woff', scheme='http')}}" rel="stylesheet" />
    <link href="{{=URL('static', 'fonts/glyphicons-halflings-regular.woff2', scheme='http')}}" rel="stylesheet" />

    <script type="text/javascript">

    $(document).ready(function(){
        $('#btn-prueba').on('click', function(){
            var a = 0;
            var strUser = document.getElementById("Hora");
            var hora = 120 - strUser.value;
            if ((hora >= 0) && (hora <= 90)) {
                a = 1;
                //alert({{estudianteId}});
                $(location).attr('href','{{=URL(r=request,f='registrarProyectoComoEstudiante',args=[proyecto.f_proyecto,estudianteId])}}'+'/'+strUser.value);
            } else {
                alert('EL número de horas debe estar comprendido entre 30 y 120')
            };

        });
    })

    </script>

  </head>

  <body role="document">
    <!-- Fixed navbar -->

    <div class="col-sm-12">
        <div class="row">
            <div class="col-md-2">
                <img src="{{=URL('static', 'estudiantes.png')}}" class="img-responsive" width="150" height="110"/>
            </div>
            <div class="col-md-6" align="left">
            <h4 style="color:#4682B4"><a href="."> Home</a> | <a href="{{=URL('vista_estudiante')}}">Estudiante</a>  | {{=proyecto.f_proyecto.f_nombre}} </h4>
            
            </div>
            <div class="col-md-4" align="right">
                <a href="{{=URL('default', 'user', args='logout')}}"><spam class="glyphicon glyphicon-log-out">Salir</spam></a>
            </div>
        
        
        </div>
        <hr>
    </div>

   <div class="col-sm-12">
       <div class="col-md-3">
           <h4 style="color:#337AB7"><i class="glyphicon glyphicon-briefcase"></i> Menú</h4>
            <hr>
             <ul class="nav nav-stacked">
             </ul>
       </div>
        {{if globals().get('msj', False):}} <p><strong>{{=msj}}</strong></p>
        {{else:}}
        <div class="col-md-5">
            <div class="row">
                <div class="panel panel-primary">
                    <div class="panel-heading class=detalles">
                       <b><h class="detalles" >Detalles del proyecto</b></h>
                    </div>
                    <div class="panel-body">
                        <table class="table table-striped">

                            
                            <tr><td><b>Código:</b> {{=proyecto.f_codigo}}</td></tr>
                            <tr><td><b>Nombre:</b> {{=proyecto.f_proyecto.f_nombre}}</td></tr>
                            <tr><td><b>Descripción:</b> {{=proyecto.f_proyecto.f_resumen}}</td></tr>
                            <tr><td><b>Área:</b> {{=proyecto.f_proyecto.f_area.f_nombre}}</td></tr>
                            <tr><td><b>Estado:</b> {{=proyecto.f_estado_proyecto}}</td></tr>
                            <tr><td><b>Fecha de Inicio:</b> {{=proyecto.f_proyecto.f_fechaini}}</td></tr>
                            <tr><td><b>Fecha de Finalización:</b> {{=proyecto.f_proyecto.f_fechafin}}</td></tr>
                            <tr><td><b>Version:</b> {{=proyecto.f_proyecto.f_sede}}</td></tr>
                            <tr><td><b>Comunidad:</b> {{=proyecto.f_proyecto.f_comunidad}}</td></tr>
                          

                        </table>
                    </div>
                </div>
            </div>
            <div id="" class="col-md-7" align="left" >
                <td><tr><b> Horas a inscribir </b></tr></td><br><br>
            </div>
            <input type="number" id="Hora" min="30" max="120" value="30">
            <button id="btn-prueba" class="btn btn-success" style="width: 240px;" align="right">
                 <i class="glyphicon glyphicon-plus-sign"></i> <b>Inscribir este proyecto</b>
            </button>
        </div>

      </div>
  {{ pass }}
{{ end }}
